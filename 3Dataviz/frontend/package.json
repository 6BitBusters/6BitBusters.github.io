{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "checkall": "cls & npm run lint & npm run format & npm run test:cov & npm run pw",
    "format": "prettier --write \"./**/*.{ts,tsx,js,glsl}\"",
    "format:check": "prettier --check \"./**/*.{ts,tsx,js,glsl}\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.{ts,tsx}\" --fix",
    "lint:check": "eslint \"{src,apps,libs,test}/**/*.{ts,tsx}\" --max-warnings=10",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "test:cov": "vitest run --coverage",
    "pw": "npx playwright test"
  },
  "dependencies": {
    "@react-three/drei": "^10.0.6",
    "@react-three/fiber": "^9.1.2",
    "@reduxjs/toolkit": "^2.6.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/three": "^0.175.0",
    "@vitest/coverage-v8": "^3.1.1",
    "fetch-mock": "^12.5.2",
    "gsap": "^3.12.7",
    "jsdom": "^26.0.0",
    "prettier": "^3.5.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-redux": "^9.2.0",
    "react-router": "^7.5.0",
    "react-select": "^5.10.1",
    "redux-mock-store": "^1.5.5",
    "redux-thunk": "^3.1.0",
    "three": "^0.175.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@playwright/test": "^1.52.0",
    "@react-three/test-renderer": "^9.0.1",
    "@types/node": "^22.14.1",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "@vitest/coverage-v8": "^3.1.1",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "jsdom": "^26.0.0",
    "react-select-event": "^5.5.1",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0",
    "vitest": "^3.1.1"
  }
}
